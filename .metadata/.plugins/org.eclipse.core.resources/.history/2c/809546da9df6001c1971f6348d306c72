package java0628;

public class ex09_구구단세로 {

	public static void main(String[] args) {
		
		for(int i=2; i<10; i++) {
			System.out.print("{"+i+"단}\t");
			}
		System.out.println();
		for(int i=1; i<10; i++) {
			for(int j=2; j<10; j++) {
				System.out.println(j+"*"+i+"="+j*i+"\t");
			}
		}

	}

}
