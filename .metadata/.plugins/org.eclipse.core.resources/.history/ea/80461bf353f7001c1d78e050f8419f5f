package java0629;

public class ex04_역삼각형별찍기 {

	public static void main(String[] args) {
		// *********
		// @*******@
		// @@*****@@
		// @@@***@@@
		// @@@@*@@@@
		for (int i = 0; i <= 4; i++) {
			for (int j = 5; j > 5 - i; j--) {
				System.out.print("^");
			}
		for(int j=0; j>5-i; j--) {
			System.out.println("^");
		}
		for(int j = 9; j>=i; j--) {
			System.out.print("*");
		}
			System.out.println();
		}
	}

}
