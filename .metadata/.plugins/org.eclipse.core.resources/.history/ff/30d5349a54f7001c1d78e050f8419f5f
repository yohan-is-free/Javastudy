package java0629;

public class ex04_역삼각형별찍기 {

	public static void main(String[] args) {
		// *********
		// @*******@
		// @@*****@@
		// @@@***@@@
		// @@@@*@@@@
		
		for (int i = 1; i <= 0; i++) {
			System.out.print("^");	
		}
		for (int i=1; i<=9; i++) {
			System.out.print("*");
		}
		System.out.println();
	}

}
