package java0629;

public class ex04_역삼각형별찍기 {

	public static void main(String[] args) {
		// *********
		// @*******@
		// @@*****@@
		// @@@***@@@
		// @@@@*@@@@
		for (int i = 1; i <= 5; i++) {
			System.out.println(" ");
			for (int j = 5; j > 5 - i; j--) {
				System.out.print("^");
			}
		for(int j = 1; j<i+1; j++) {
			System.out.print("*");
		}
			
		}
	}

}
