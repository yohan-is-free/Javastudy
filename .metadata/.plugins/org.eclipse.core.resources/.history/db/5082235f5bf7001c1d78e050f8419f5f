package java0629;

public class ex05_약수구하기 {

	public static void main(String[] args) {

		// 2~30까지의 약수를 구하세요

		System.out.print("5의 약수 : ");
		for (int i = 1; i <= 5; i++) {
			System.out.print(i + " ");
			for(int j=1; j<=i; j++) {
				if(i%j==0) {
					System.out.print("");
				}
				System.out.println();
			}
		}

	}
}
