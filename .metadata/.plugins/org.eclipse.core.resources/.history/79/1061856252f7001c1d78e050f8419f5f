package java0629;

public class ex04_역삼각형별찍기 {

	public static void main(String[] args) {
		//*********
		//@*******@
		//@@*****@@
		//@@@***@@@
		//@@@@*@@@@
		for(int i=0; i<=9; i++) {
			for(int j=8; j>8-i;j--) {
				for(int a=8;a<8+i;a++) {
					System.out.print("^");
					
				}
				System.out.println();
			}
		}

	}

}
